{
  "name": "{{ language.params.npmPackage }}",
  "homepage": "{{ spec.contactURL }}",
  "description": "{{ sdk.shortDescription }}",
  "version": "{{ sdk.version }}",
  "license": "{{ sdk.license }}",
  "main": "./dist/cjs/sdk.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/sdk.js",
      "require": "./dist/cjs/sdk.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/mjs/sdk.js",
  "repository": {
    "type": "git",
    "url": "{{ sdk.gitURL }}"
  },
  "scripts": {
    "build": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json"
  },
  "devDependencies": {
    "playwright": "^1.10.0",
    "typescript": "^4.2.3",
    "serve-handler": "^6.1.3"
  },
  "dependencies": {}
}