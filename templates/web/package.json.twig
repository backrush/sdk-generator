{
  "name": "{{ language.params.npmPackage }}",
  "homepage": "{{ spec.contactURL }}",
  "description": "{{ sdk.shortDescription }}",
  "version": "{{ sdk.version }}",
  "license": "{{ sdk.license }}",
  "main": "./dist/cjs/sdk.js",
  "exports": {
    ".": {
      "import": "./dist/esm/sdk.js",
      "require": "./dist/cjs/sdk.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/esm/sdk.js",
  "repository": {
    "type": "git",
    "url": "{{ sdk.gitURL }}"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:umd",
    "build:esm": "tsc --module es6 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:umd": "rollup dist/esm/sdk.js --format umd --name Appwrite --context window -m --no-esModule -o dist/umd/sdk.js"
  },
  "devDependencies": {
    "playwright": "^1.10.0",
    "rollup": "^2.43.0",
    "serve-handler": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {}
}
