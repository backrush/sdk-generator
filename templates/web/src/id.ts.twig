/**
 * Helper class to generate ID strings for resources.
 */
export class ID {
    /**
     * Uses the provided ID as the ID for the resource.
     *
     * @param {string} id
     * @returns {string}
     */
    static custom(id: string): string {
        return id;
    }

    /**
     * Have Appwrite generate a unique ID for you.
     *
     * @returns {string}
     */
    static unique(): string {
        return 'unique()';
    }
}
