class Role {
    static any = () => {
        return 'any'
    }
    static user = (id) => {
        return 'user:' + id
    }
    static users = () => {
        return 'users'
    }
    static guests = () => {
        return 'guests'
    }
    static team = (id, role = '') => {
        if(role === '') {
            return 'team:' + id
        }
        return 'team:' + id + '/' + role
    }
    static status = (status) => {
        return 'status:' + status
    }
}

module.exports = Role;