class Permission:

    @staticmethod
    def read(role) -> str:
        return f'read("{role}")'

    @staticmethod
    def write(role) -> str:
        return f'write("{role}")'

    @staticmethod
    def create(role) -> str:
        return f'create("{role}")'

    @staticmethod
    def update(role) -> str:
        return f'update("{role}")'

    @staticmethod
    def delete(role) -> str:
        return f'delete("{role}")'
