import 'package:http/http.dart' show MultipartFile, encoding;

class InputFile {
  final MultipartFile? file;
  final String? path;
  final String? filename;
  final List<int>? bytes;
  final String? contentType;

  /// Type definition for file parameters
  /// @deprecated @param file use `InputFile.fromMultipartFile(file)` instead
  InputFile({this.path, this.filename, @deprecated this.file});

  /// Use only for Files below 5 MB in size
  InputFile.fromMultipartFile(this.file);

  InputFile.fromBytes(this.bytes, { this.filename, this.contentType });

}
