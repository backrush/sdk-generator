        return client.call(
            "{{ method.method | caseUpper }}",
            apiPath,
            headers,
            params,
            {%~ if method.responseModel | hasGenericType(spec) %}
            responseType = classOf(),
            {%~ else %}
            responseType = {{ method | returnType(spec, sdk.namespace | caseDot) | raw }}::class.java,
            {%~ endif %}
            {%~ if method.responseModel %}
            converter,
            {%~ endif %}
        )