package {{ sdk.namespace | caseDot }}

class Permissions {
    companion object {
        fun read(var role: String): String {
            return "read(\"${role}\")"
        }        
        fun write(var role: String): String {
            return "write(\"${role}\")"
        }        
        fun create(var role: String): String {
            return "create(\"${role}\")"
        }        
        fun update(var role: String): String {
            return "update(\"${role}\")"
        }        
        fun delete(var role: String): String {
            return "delete(\"${role}\")"
        }        
    }
}