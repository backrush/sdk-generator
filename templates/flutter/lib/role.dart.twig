part of {{ language.params.packageName }};

class Role {
    static String any() {
        return 'any';
    }

    static String user(String id) {
        return 'user:$id';
    }

    static String users() {
        return 'users';
    }

    static String guests() {
        return 'guests';
    }

    static String team(String id, [String? role]) {
        if(role != null) {
            return 'team:$id/$role';
        }
        return 'team:$id';
    }
}